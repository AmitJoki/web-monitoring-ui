{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
      "arrow-parens": [true, "ban-single-arg-parens"],
      "quotemark": [true, "single", "jsx-double", "avoid-escape"],
      "member-access": [true, "no-public"],
      "one-line": [false],
      "space-before-function-paren": [true, "always"],
      "trailing-comma": [false]
    },
    "rulesDirectory": []
}
